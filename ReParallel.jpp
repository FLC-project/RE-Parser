     */

    private boolean recParFwd(Counter cnt, int P, int[][] L){
        TRACE(Q,"recParFwd\n");
        for (;;){
            int pi = cnt.get();
            if (pi < 0) break;
            if (!recParFwd(pi,P,L)) return false;
